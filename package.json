{
  "name": "starter-ts",
  "version": "1.0.2",
  "description": "",
  "keywords": [],
  "scripts": {
    "dev": "rollup -c -w",
    "build": "rollup -c && tsc -p ./tsconfig.json",
    "lint": "eslint . --fix",
    "format": "prettier --write .",
    "prepare": "husky install",
    "commit": "cz",
    "test": "jest"
  },
  "type": "module",
  "main": "./dist/index.mjs",
  "types": "./dist/types/index.d.ts",
  "exports": {
    "import": "./dist/index.mjs",
    "require": "./dist/index.cjs",
    "types": "./dist/types/index.d.ts"
  },
  "author": "Quincy Qiu",
  "license": "ISC",
  "devDependencies": {
    "@commitlint/cli": "^17.4.4",
    "@commitlint/config-conventional": "^17.4.4",
    "@jest/globals": "^29.4.3",
    "@qiuqfang/eslint-config": "^0.0.6",
    "@qiuqfang/rollup-plugin-swc": "^0.0.9",
    "@rollup/plugin-commonjs": "^24.0.1",
    "@rollup/plugin-json": "^6.0.0",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "commitizen": "^4.3.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.36.0",
    "husky": "^8.0.3",
    "jest": "^29.4.3",
    "lint-staged": "^13.1.2",
    "prettier": "^2.8.7",
    "rollup": "^3.15.0",
    "rollup-plugin-delete": "^2.0.0",
    "ts-jest": "^29.0.5",
    "typescript": "^5.0.0"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
      "eslint --fix"
    ],
    "src/**/*.*": [
      "prettier --write"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
